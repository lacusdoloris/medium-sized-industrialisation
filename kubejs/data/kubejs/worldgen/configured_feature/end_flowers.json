{
    "type": "bigglobe:script",
    "config": {
        "script": {
            "debug_name": "EndFlowers",
            "source": [
                "random.unless(1.0/4.0: return(false))",

                "Block flower = BlockTag('nijika:idyll_flowers').random()",
                "if (flower.getDefaultState().canPlaceAt(originX, originY, originZ):",
                    "setBlockState(originX, originY, originZ, flower.getDefaultState())",
                ")",

                "return(true)"
            ]
        }
    }
}
