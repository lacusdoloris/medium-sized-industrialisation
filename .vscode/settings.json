{
    "json.schemas": [
        {
            "fileMatch": [
                "/lang/*.json"
            ],
            "url": "./.vscode/probe.lang-schema.json"
        },
        {
            "fileMatch": [
                "/probe/docs/*.json"
            ],
            "url": "./.vscode/probe.doc-schema.json"
        }
    ],
    "files.exclude": {
        "**/node_modules": true
    },
    "eslint.lintTask.enable": true,
    "licenser.author": "Lura Skye",
    "licenser.license": "MPLv2",
    "licenser.projectName": "Bigger Industrialisation",
    "esbonio.server.pythonPath": ".venv/bin/python3",
    "python.defaultInterpreterPath": ".venv/bin/python3",
    "esbonio.sphinx.pythonCommand": [".venv/bin/python3"]
}
