{
    "name": "bigger-industry",
    "version": "0.7.0",
    "description": "bi modpack",
    "repository": "https://github.com/Fuyukai/Bigger-Industrialisation.git",
    "author": "Lura Skye <l@veriny.tf>",
    "license": "MPL-2.0",
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.22.8",
        "@babel/plugin-transform-classes": "^7.22.6",
        "@babel/plugin-transform-new-target": "^7.22.5",
        "@babel/plugin-transform-numeric-separator": "^7.18.6",
        "@babel/plugin-transform-runtime": "^7.22.7",
        "eslint": "^8.57.0",
        "eslint-plugin-jsdoc": "^48.2.1",
        "parcel": "^2.9.3",
        "prettier": "3.2.5"
    },
    "scripts": {
        "build-prod": "parcel build",
        "build-dev": "parcel build --no-optimize"
    },
    "targets": {
        "server": {
            "source": "./src/server/index.js",
            "distDir": "./kubejs/server_scripts",
            "outputFormat": "global",
            "sourceMap": false,
            "engines": {
                "browsers": "firefox > 90"
            }
        },
        "startup": {
            "source": "./src/startup/index.js",
            "distDir": "./kubejs/startup_scripts",
            "outputFormat": "global",
            "sourceMap": false,
            "engines": {
                "browsers": "firefox > 90"
            }
        },
        "client": {
            "source": "./src/client/index.js",
            "distDir": "./kubejs/client_scripts",
            "outputFormat": "global",
            "sourceMap": false,
            "engines": {
                "browsers": "firefox > 90"
            }
        }
    }
}
