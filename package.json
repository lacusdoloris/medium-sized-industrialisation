    "license": "MPL-2.0",
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/plugin-transform-classes": "^7.25.9",
        "@babel/plugin-transform-new-target": "^7.25.9",
        "@babel/plugin-transform-numeric-separator": "^7.25.9",
        "@babel/plugin-transform-runtime": "^7.25.9",
